{
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Functions/create_oauth_client.php",
            "app/Helpers/Functions/slug_uri.php",
            "app/Helpers/Functions/slug.php",
            "app/Helpers/Functions/user_email_verified.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "description": "A Content Management System",
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "keywords": [
        "CMS",
        "Content Management System"
    ],
    "license": "MIT",
    "minimum-stability": "beta",
    "name": "zero-to-prod/cms",
    "prefer-stable": true,
    "require": {
        "aws/aws-sdk-php": "~3.0",
        "php": "^7.2.5",
        "ext-json": "*",
        "ext-pdo": "*",
        "cloudcreativity/laravel-json-api": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-frontend-presets/tall": "^1.7",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.3",
        "laravel/passport": "^9.2",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^1.0",
        "neomerx/json-api": "^1.0.3",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "cloudcreativity/json-api-testing": "^3.0",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "type": "project"
}
